<Window x:Class="Client.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Client"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Margin="10" Height="100">
            <Button Content="Products" Width="100" Click="ProductsButton_Click"/>
            <Button Content="Manufacturers" Width="100" Click="ManufacturersButton_Click" Margin="50,0,0,0"/>
        </StackPanel>
    </Grid>
</Window>
